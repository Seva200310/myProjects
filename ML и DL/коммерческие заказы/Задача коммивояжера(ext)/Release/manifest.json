{
  "name": "Коммивояжер",
  "version": "0.9",
  "manifest_version": 3,
  "description": "Расширение для решения задачи коммивояжера",
  "icons": {
    "16": "icons/businessdelay16-16.png",
    "32": "icons/businessdelay32-32.png",
    "48": "icons/businessdelay48-48.png",
    "128": "icons/businessdelay128-128.png"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "contextMenus",
    "tabs",
    "storage",
    "activeTab"
  ]
}
